{{! template-lint-disable no-implicit-this }}
{{#if renderTooltip}}
  {{#in-element destinationEl insertBefore=null}}
    <div class="md-panel-outer-wrapper md-panel-is-showing" style={{containerStyle}}>
      <PaperTooltipInner @class={{this.class}} @position={{this.position}} @anchorElement={{this.anchorElement}} @hide={{this.hideTooltip}} ...attributes>
        {{yield}}
      </PaperTooltipInner>
    </div>
  {{/in-element}}
{{/if}}
